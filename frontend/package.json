{
  "name": "psn-pulse-frontend",
  "version": "1.0.0",
  "description": "PSN Pulse Frontend - Svelte Dashboard for PSN Migration Tracking",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:widget": "vite build --config vite.widget.config.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .svelte,.js,.ts",
    "format": "prettier --write .",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "dependencies": {
    "svelte": "^4.2.8",
    "svelte-routing": "^2.0.0",
    "svelte-spa-router": "^4.0.0",
    "chart.js": "^4.4.0",
    "date-fns": "^2.30.0",
    "lucide-svelte": "^0.294.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.32",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.1.0",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0"
  },
  "keywords": [
    "psn",
    "pulse",
    "svelte",
    "dashboard",
    "healthcare",
    "migration"
